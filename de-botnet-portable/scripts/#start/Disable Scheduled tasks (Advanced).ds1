Task14=Run,schtasks /Change /TN "Microsoft\Windows\DiskFootprint\Diagnostics" /disable,STDOUT
Task15=Run,schtasks /Change /TN "Microsoft\Windows\FileHistory\File History (maintenance mode)" /disable,STDOUT
Task16=Run,schtasks /Change /TN "Microsoft\Windows\Maintenance\WinSAT" /disable,STDOUT
Task17=Run,schtasks /Change /TN "Microsoft\Windows\PI\Sqm-Tasks" /disable,STDOUT
Task18=Run,schtasks /Change /TN "Microsoft\Windows\Power Efficiency Diagnostics\AnalyzeSystem" /disable,STDOUT
Task19=Run,schtasks /Change /TN "Microsoft\Windows\Shell\FamilySafetyMonitor" /disable,STDOUT
Task20=Run,schtasks /Change /TN "Microsoft\Windows\Shell\FamilySafetyRefresh" /disable,STDOUT
Task21=Run,schtasks /Change /TN "Microsoft\Windows\Shell\FamilySafetyUpload" /disable,STDOUT
