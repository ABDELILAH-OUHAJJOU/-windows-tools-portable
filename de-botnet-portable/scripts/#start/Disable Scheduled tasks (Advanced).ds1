Task23=Run,schtasks /Change /TN "Microsoft\Windows\WindowsUpdate\Automatic App Update" /disable,STDOUT
Task24=Run,schtasks /Change /TN "Microsoft\Windows\License Manager\TempSignedLicenseExchange" /disable,STDOUT
Task25=Run,schtasks /Change /TN "Microsoft\Windows\Clip\License Validation" /disable,STDOUT
Task26=Run,schtasks /Change /TN "Microsoft\Windows\ApplicationData\DsSvcCleanup" /disable,STDOUT
Task27=Run,schtasks /Change /TN "Microsoft\Windows\Power Efficiency Diagnostics\AnalyzeSystem" /disable,STDOUT
Task28=Run,schtasks /Change /TN "Microsoft\Windows\PushToInstall\LoginCheck" /disable,STDOUT
Task29=Run,schtasks /Change /TN "Microsoft\Windows\PushToInstall\Registration" /disable,STDOUT
Task30=Run,schtasks /Change /TN "Microsoft\Windows\Shell\FamilySafetyMonitor" /disable,STDOUT
Task31=Run,schtasks /Change /TN "Microsoft\Windows\Shell\FamilySafetyMonitorToastTask" /disable,STDOUT
Task32=Run,schtasks /Change /TN "Microsoft\Windows\Shell\FamilySafetyRefreshTask" /disable,STDOUT
Task33=Run,schtasks /Change /TN "Microsoft\Windows\Subscription\EnableLicenseAcquisition" /disable,STDOUT
Task34=Run,schtasks /Change /TN "Microsoft\Windows\Subscription\LicenseAcquisition" /disable,STDOUT
Task35=Run,schtasks /Change /TN "Microsoft\Windows\Diagnosis\RecommendedTroubleshootingScanner" /disable,STDOUT
Task36=Run,schtasks /Change /TN "Microsoft\Windows\Diagnosis\Scheduled" /disable,STDOUT
Task38=Run,schtasks /Change /TN "Microsoft\Windows\NetTrace\GatherNetworkInfo" /disable,STDOUT
File1=Run,del /F /Q "C:\Windows\SysNative\Tasks\Microsoft\Windows\SettingSync\*"
File2=Run,del /F /Q "C:\Windows\System32\Tasks\Microsoft\Windows\SettingSync\*"

